<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.patrick.mytweet">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="app.myTweet.main.MyTweetApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="app.myTweet.activity.Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="app.myTweet.activity.Signup"
            android:label="@string/signupTitle"></activity>
        <activity
            android:name="app.myTweet.activity.Login"
            android:label="@string/buttonLogin"></activity>
        <activity android:name="app.myTweet.activity.MyTweet"
            
            android:label="@string/app_name">

        </activity>
        <activity
            android:name="app.myTweet.activity.Newsfeed"
            android:label="@string/app_name"></activity>
        <activity
            android:name="app.myTweet.activity.tweetview"
            android:label="@string/app_name">
            <!--<meta-data android:name="android.support.PARENT_ACTIVITY"-->
                <!--android:value="app.myTweet.activity.Newsfeed"/>-->
        </activity>



    </application>

</manifest>